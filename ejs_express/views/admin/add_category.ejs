<%- include('../partials/admin_header'); %>
		
		<%messages.forEach(function(message) {%>

			<div class="alert alert-warning alert-dismissible fade show" role="alert">
			  <strong><%= message %></strong> 
			  <button type="button" class="close" data-dismiss="alert" aria-label="Close">
			    <span aria-hidden="true">&times;</span>
			  </button>
			</div>

		<%})%>	

		
		

	
		<h3 class="page-title">category</h3>
		<a href="/admin/pages" class="btn btn-primary"> back to all pages</a>

		<form action="/category/new" method="post" style="margin: 20px"> 
			  <div class="form-group">
			    <label for="title">Enter category title</label>
			    <input type="text" name="title" class="form-control"  aria-describedby="pageTitle" placeholder="Enter title.." value="<%=title %>">
			    <small  class="form-text text-muted">enter category title</small>
			  </div>

			  		  
			 
			  <button type="submit" class="btn btn-primary">Submit</button>
		</form>

				




