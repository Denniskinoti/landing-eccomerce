<%- include('../partials/header'); %>

<div>
<%product.forEach(function(product){%>
	<div class="col-xs-12 col-md-4 col-lg-3 p" >
		<a class="pa" href="/produucts/<%=product.category%>/<%=product.slug%>">
			<img class="pimage" src="/productImage/<%=product.id%>/<%=product.image%>">
		</a>

		<h4><%= product.title %></h4>
		<h3>$<%= parseFloat(product.price).toFixed(2) %> </h3>
		<a class="vd" href="/produucts/<%=product.category%>/<%=product.slug%>">view mores</a>

	</div>

<%})%>
</div> 

	
